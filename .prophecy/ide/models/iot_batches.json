        "macroDependencies" : [ ],
        "comment" : "Tracks and reports the number of IoT records loaded over time.",
        "autoUpdateComment" : true,
        "isLabelGrayed" : false
      },
      "properties" : {
        "customQueryDisabled" : false,
        "customQuery" : true,
        "incrementalEditorDisabled" : true,
        "query" : "WITH iot_incremental AS (\n\n  SELECT *\n  \n  FROM IOT\n  \n  {% if is_incremental() %}\n    WHERE \n      LOADED_AT > (\n        SELECT COALESCE( MAX(LOADED_AT), '1900-01-01')\n        FROM IOT_BATCHES\n  {% endif %}\n\n),\n\ncount_by_date AS (\n\n  SELECT \n    LOADED_AT AS LOADED_AT,\n    DATE(ts) AS DATE,\n    COUNT(*) AS N_RECORDS_LOADED\n  \n  FROM iot_incremental\n  \n  GROUP BY \n    LOADED_AT, DATE(TS)\n\n),\n\nreported_at AS (\n\n  SELECT \n    LOADED_AT,\n    DATE,\n    N_RECORDS_LOADED,\n    current_timestamp() AS REPORTED_AT\n  \n  FROM count_by_date\n\n)\n\nSELECT *\n\nFROM reported_at\n\n",
        "incrementalKey" : false,
        "incremental" : {
          "expression" : "true"
        }
